{
  "name": "octopoda-tanstack-start",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --bun run vinxi dev",
    "build": "bun --bun run vinxi build",
    "start": "bun --bun run vinxi start",
    "after-install": "bun i && bun db:migrate",
    "inspect": "bun --inspect --bun run vinxi dev",
    "format:check": "bun --bun prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "bun --bun prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "db:generate": "bun --bun node_modules/.bin/drizzle-kit generate",
    "db:migrate": "bun --bun node_modules/.bin/drizzle-kit migrate",
    "db:push": "bun --bun node_modules/.bin/drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:dbml": "bun drizzle/generate-dbml.ts",
    "lint": "bun --bun eslint \"**/*.{ts,tsx,js,jsx,mdx}\"",
    "lint:fix": "bun --bun eslint \"**/*.{ts,tsx,js,jsx,mdx}\" --fix",
    "typecheck": "bun --bun tsc --noEmit",
    "e2e-ui": "bun playwright test --ui "
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
